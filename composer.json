{
    "name": "heimrichhannot/contao-dynamicfeed-bundle",
    "type": "contao-bundle",
    "description": "Contao 4 dynamic feed bundle",
    "license": "LGPL-3.0-or-later",
    "require": {
        "php": "~7.4 || ^8.0",
        "contao/core-bundle": "~4.9",
        "symfony/routing": "^4.4 || ^5.4"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\ContaoDynamicFeedBundle\\": "src/"
        },
        "classmap": [
            "src/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\ContaoDynamicFeedBundle\\ContaoManager\\Plugin"
    }
}
